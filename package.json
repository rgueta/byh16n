{
  "cia": "Bytheg",
  "name": "byh16n",
  "version": "3.0.0",
  "description": "byh16 backend nodejs",
  "main": "dist/index.js",
  "engines": {
    "node": "14.17.5",
    "npm": "6.14.14"
  },
  "scripts": {
    "clean_modules": "rm -f ./package-lock.json && rm -rf node_modules && npm cache clean --force && npm install",
    "clean_": "rm -rf dist/ && npm run clean_modules",
    "clean": "rm -rf dist/",
    "build_": "babel src -d dist",
    "build": "npm run clean && babel src --out-dir dist --source-maps --copy-files",
    "production": "npm run build && node bin/production",
    "build_bk": "babel src --out-dir build",
    "dev": "nodemon --exec babel-node src/index.js",
    "start_as_dev": "node --exec babel-node build/index.js",
    "start_build": "npm run build && node dist/index.js",
    "start_simple": "node dist/index.js",
    "start": "npm run build && node dist/index.js",
    "aws" : "node src/index.js"
  },
  "keywords": [],
  "author": "Ricardo Gueta",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/node": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/register": "^7.17.7",
    "@types/nodemailer": "^6.4.4",
    "aws-sdk": "^2.1114.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-fileupload": "^1.4.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.0",
    "sharp": "^0.29.3",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "twilio": "^3.71.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
